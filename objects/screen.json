{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "type",
    "id",
    "height",
    "width"
  ],
  "properties": {
    "type": {
      "$id": "#/properties/type",
      "type": "string",
      "enum": [
        "screen"
      ],
      "title": "The Type Schema",
      "examples": [
        "screen"
      ],
      "pattern": "^(.*)$"
    },
    "id": {
      "$id": "#/properties/id",
      "type": "integer",
      "title": "The Id Schema",
      "examples": [
        1
      ],
      "minimum": 1.0
    },
    "height": {
      "$id": "#/properties/height",
      "type": "integer",
      "title": "The Height Schema",
      "default": 0,
      "examples": [
        100
      ],
      "minimum": 1.0
    },
    "width": {
      "$id": "#/properties/width",
      "type": "integer",
      "title": "The Width Schema",
      "default": 0,
      "examples": [
        100
      ],
      "minimum": 1.0
    }
  }
}
